{
	"name": "fetch-addons",
	"version": "1.1.0",
	"description": "A collection of addons for the fetch API",
	"license": "MIT",
	"repository": "smeijer/fetch-addons",
	"funding": "https://github.com/smeijer/fetch-addons?sponsor=1",
	"author": {
		"name": "Stephan Meijer",
		"email": "stephan.meijer@gmail.com",
		"url": "https://meijer.ws"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.cjs",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./*.js": {
			"import": "./dist/*.js",
			"types": "./dist/*.d.ts"
		}
	},
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"test": "prettier -w . && eslint . --fix && jest",
		"build": "rimraf ./dist && tsc -p tsconfig.build.json && npm run build:cjs",
		"build:cjs": "esbuild src/index.ts --bundle --outfile=dist/index.cjs --format=cjs --platform=node",
		"prepare": "npm run build"
	},
	"files": [
		"dist",
		"license"
	],
	"keywords": [
		"fetch",
		"fetch-api",
		"fetch-addons",
		"fetch-helpers",
		"fetch-utils",
		"curl",
		"fetch-to-curl"
	],
	"devDependencies": {
		"@smeijer/eslint-config": "^1.1.0",
		"@smeijer/tsconfig": "^1.0.0",
		"@types/jest": "^29.5.2",
		"esbuild": "^0.18.11",
		"jest": "^29.5.0",
		"rimraf": "^5.0.1",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	}
}
